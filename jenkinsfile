pipeline {
    agent any
    stage('Check Node.js') {
    steps {
        sh 'node --version'
        sh 'npm --version'
    }
}
    stages {
        stage('Install Dependencies') {
            steps {
                sh 'npm install'
            }
        }

        stage('Deploy to Render') {
            steps {
                sh 'node server.js'
            }
        }
    }
}


